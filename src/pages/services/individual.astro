---
import StructuredData from '../../components/StructuredData.astro';
import Layout from '../../layouts/Layout.astro';
import { canonicalFor } from '../../lib/siteConfig';
const serviceData = {
  '@context': 'https://schema.org',
  '@type': 'Service',
  name: 'Individual Therapy',
  url: canonicalFor(Astro.url.pathname),
  description: 'Personalized one-on-one psychotherapy sessions using evidence-based modalities.'
};
---
<Layout title="Individual Therapy" description="Overview of one-on-one individual therapy services.">
  <Fragment slot="head">
    <StructuredData data={serviceData} />
  </Fragment>
  <h1>Individual Therapy</h1>
  <p>Placeholder: Description of individualized therapeutic process, goals, typical session length, and approach (e.g., CBT/ACT integration).</p>
</Layout>
